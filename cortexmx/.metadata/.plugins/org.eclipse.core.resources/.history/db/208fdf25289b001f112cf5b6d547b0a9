#include <stdint.h>

//#if !defined(__SOFT_FP__) && defined(__ARM_FP)
//  #warning "FPU is not initialized, but the project is compiling for an FPU. Please initialize the FPU before use."
//#endif

#define USART2_IRQ	38
int main(void)
{
	/*manually pend the USART2 interrupt in ISPR1*/
	uint32_t *pISPR = (uint32_t*)0XE000E204U;
	*pISPR |= (1 << (USART2_IRQ % 32));

	/*enable the USART2 interrupt on processor side on ISER1 */
	uint32_t *pISER = (uint32_t*)0xE000E100U;
	*pISER = (1 << (USART2_IRQ % 32));
    /* Loop forever */
	for(;;);
}
